{
  "name": "ossystem.com.ua",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && webpack --env production --config ./webpack.config/client.config.js --progress --profile --colors --display-error-details",
    "build:admin": "npm run clean && webpack --env production --config ./webpack.config/admin.config.js --progress --profile --colors --display-error-details",
    "deploy": "npm run build && npm run server:deploy",
    "migrate": "NODE_ENV=production node ./server/migrations/index.js",
    "start": "webpack-dev-server --config ./webpack.config/client.config.js --host 0.0.0.0",
    "start:admin": "webpack-dev-server --config ./webpack.config/admin.config.js  --host 0.0.0.0",
    "server": "./node_modules/.bin/nodemon --ignore './src/' ./server/index.js",
    "server:deploy": "node ./node_modules/.bin/pm2 kill && NODE_ENV=production node ./node_modules/.bin/pm2 start ./server/index.js --name ossystem -i max",
    "lint:eslint": "eslint --ext=.jsx --ext=.js ./src/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:js": "linting-rules js",
    "lint:sass": "linting-rules sass",
    "lint": "npm run lint:js && npm run lint:sass"
  },
